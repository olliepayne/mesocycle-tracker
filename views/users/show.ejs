<%- include('../partials/header.ejs') %>

<h1>You</h1>
<section>
  <% if(userModel.mesocycles.length > 0) { %>
    <h3>userModel.mesocycles[userModel.mesocycles.length - 1].attribute</h3>
  <% } else { %>
    <h3>No Mesocycles...</h3>
  <% } %>
  <form action="/users/<%= user._id %>/mesocycles" method="GET">
    <button>View</button>
  </form>
  <form action="" method="DELETE">
    <button>End</button>
  </form>
</section>
<section>
  <h3><%= userModel.mesocycles.length > 0 ? 'Add a Mesocycle!' : 'Last Session' %></h3>
</section>

<%- include('../partials/footer.ejs') %>